	<block wx:if="{{timetableMode == 1}}">
		<swiper class="main" bindchange="bindchangeday" duration="300" easing-function="ease-in-out" circular="true"
			bindtap="collapse">
			<swiper-item class="main1">
				<daily list="{{course_data}}" id="the-id" bindcustomevent="getName"></daily>
			</swiper-item>
			<swiper-item class="main1">
				<daily list="{{course_data}}" bindcustomevent="getName"></daily>
