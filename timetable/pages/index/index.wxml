<navBar background='#fff' back="{{true}}" home="{{true}}" bindback="handlerGobackClick" bindhome="handlerGohomeClick" />
<view class="background">
	<swiper class="header" bindchange="bindchange" duration="500" easing-function="east-out" circular="true">
		<swiper-item>
			<view class="date">
				{{date[6].year}}年{{date[6].month}}月
				<view class="set">
					<image src="../../assets/icons/calendar.png" class="icon" mode="aspectFit" bindtap="tapActivity" />
					<navigator url="../derivate/derivate" hover-class="none">
						<image src="../../assets/icons/export.png" class="icon" mode="aspectFit" />
					</navigator>
					<image src="../../assets/icons/refresh.png" class="icon" mode="aspectFit" bindtap="refresh" />
					<!-- <image src="../../assets/icons/set5.png" class="icon" bindtap="tapSet"></image> -->
				</view>
			</view>
			<view class="scroll_time">
				<view class="this_week" bindtap="tapSet">
					<view>{{this_week}}</view>
					<text>周</text>
				</view>
				<view class="choose_time">
					<view class="days {{choosedday.week == days.week? 'choose_couple':''}}" wx:for="{{date}}"
						wx:for-index="day" wx:key="day" wx:for-item="days" data-days="{{days}}" bindtap="tapDays">
						<view class="day">{{days.day}}</view>
						<view class="week">{{days.weeks}}</view>
					</view>
				</view>
			</view>
		</swiper-item>
