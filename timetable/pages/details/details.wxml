<!--timetable/pages/details/details.wxml-->
<navBar background="#FFF" back="true" home="true" bindback="navBack" bindhome="navHome" />
<view class="details">
<view class="courseName">
  <text>课程名称：{{showCourse}}</text>
</view>
<view class="time">
  <text>课程时间：{{showTime}}</text>
</view>
