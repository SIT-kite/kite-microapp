# https://EditorConfig.org

root = true

# For *.json and *.wxss
[*]
indent_style = tab
indent_size = 4
# tab_width = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.wxml]
indent_size = 1

[*.js]
indent_style = space
indent_size = 2

[project{,.private}.config.json]
indent_style = space
indent_size = 2

[todo.txt]
indent_size = 8
